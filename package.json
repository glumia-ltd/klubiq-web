{
  "name": "klubiq-web",
  "version": "0.0.1",
  "description": "klubiq-web project",
  "private": true,
  "author": "Glumia Solutions",
  "scripts": {
    "buildUIC": "turbo build --filter=./packages/ui-components",
    "storybook": "turbo storybook --filter=./packages/ui-components",
    "devUIC": "turbo dev --filter=./packages/ui-components",
    "buildLP": "turbo build --filter=./apps/landlord-portal",
    "buildTP": "turbo build --filter=./apps/tenant-portal",
    "devLP": "turbo dev --filter=./apps/landlord-portal",
    "devTP": "turbo dev --filter=./apps/tenant-portal",
    "lintLP": "turbo lint --filter=./apps/landlord-portal",
    "lintTP": "turbo lint --filter=./apps/tenant-portal",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "node .husky/install.mjs",
    "version:tenant": "node scripts/generate-version.js tenant-portal",
    "version:landlord": "node scripts/generate-version.js landlord-portal",
    "version:admin": "node scripts/generate-version.js admin-portal",
    "version:all": "node scripts/update-all-versions.js"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/redux-persist": "^4.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.5.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.14.0",
    "terser": "^5.42.0",
    "turbo": "^2.4.4",
    "vite": "^5.1.0",
    "vite-plugin-pwa": "^0.19.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.5.0",
  "workspaces": [
    "apps/landlord-portal",
    "apps/admin-portal",
    "apps/tenant-portal",
    "packages/*"
  ],
  "dependencies": {
    "@mui/x-data-grid": "^7.13.0",
    "@mui/x-date-pickers": "^7.29.1",
    "date-fns": "^3.6.0",
    "framer-motion": "^12.9.2",
    "react-qr-code": "^2.0.15",
    "redux-persist": "^6.0.0"
  },
  "peerDependencies": {
    "vite": "^6.3.5"
  }
}
